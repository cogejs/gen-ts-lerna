---
to: <%- drd || 'packages' %>/<%- projectName %>/package.json
---
{
  "name": "<%- name %>",
  "description": "<%- description %>",
  "version": "0.1.0",
  "keywords": [
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "author": "<%- author %> ",
  "license": "<%- license %> ",
  "scripts": {
    "acceptance": "tib-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "tib-tsc",
    "clean": "tib-clean <%-archiveName%>*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "tib-mocha \"dist/__tests__/**/*.js\"",
    "unit": "tib-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf <%-archiveName%>*.tgz && tree package && npm run clean"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@tib/build": "^6.3.0",
    "@tib/eslint-config": "^8.2.0",
    "@tib/testlab": "^3.4.0",
    "@types/node": "^14.0.26"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ]
}
